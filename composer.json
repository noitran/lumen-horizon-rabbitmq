{
    "name": "noitran/lumen-horizon-rabbitmq",
    "description": "Connector package between horizon and RabbitMQ for lumen framework",
    "keywords": [
        "lumen",
        "lumen-horizon",
        "lumen-horizon-rabbitmq",
        "rabbitmq"
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.7",
        "kinsolee/horizon-lumen": "^1.2",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^7.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "illuminate/events": "^5.7",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Noitran\\Lumen\\Horizon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Noitran\\Lumen\\Horizon\\Tests\\": "tests"
        }
    }
}
